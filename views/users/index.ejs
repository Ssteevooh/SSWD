<h2>Users Table</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>User name</th>
      <th>Edit</th>
      
    </tr>
  </thead>
  <tbody>
    <% user.forEach(user => { %>
    <tr>
      <td>
        <a href="<%= `/users/${user._id}`%>">
          <%= user.fullname %>
        </a>
      </td>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= user.username %>
      </td>
      <td>
        <a class= "btn btn-info w-10" href="<%=`/users/${user._id}/edit`%>">
          Edit
        </a>
      </td>
      <td>
        <a class= "btn btn-danger w-10" href="<%=`/users/${user._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this User?')">Delete</a>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>